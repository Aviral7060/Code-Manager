{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/v1/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://winter-school-for-iciss.vercel.app"
  },
  "public": true
}
