{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/src/index.js"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://winter-school-for-iciss.vercel.app/" 
  },
  "public": true
}
